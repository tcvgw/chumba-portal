<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d %-5p [%t] %l - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="spark" level="trace" />
		<Logger name="co.vgw" level="debug" />
		<Root level="info">
			<AppenderRef ref="STDOUT" />
		</Root>
			
	</Loggers>
</Configuration>